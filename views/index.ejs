<head>
    <title>Beill's Greenhouse - Home</title>
    <script defer src="/scripts/viewSpecific/index.js"></script>
</head>

<body>

    <p>Home page</p>

    <div class="recipesDisplayContainer">
    <% if (recipes) { %>
        <% recipes.forEach(recipe => { %>
            <div class="recipe">
                <div class="recipeImage">
                <img src="/images/burger.jpg" width="100" height="100">
                </div>
                <a class="recipeTitle">
                    <%= recipe.title %>
                </a>
            </div>
        <% })} %>
    </div>
</body>